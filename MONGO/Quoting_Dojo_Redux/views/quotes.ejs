
<html>
  <head>
    <meta charset="utf-8">
    <title>QuotingDojo</title>
  </head>
  <body>
      <fieldset>
        <legend><h2>Here are the awsome quotes!</h2></legend>
        <% for(var i in quotes){ %>
        <h3>Author:<%= quotes[i].name %></h3>
        <h5>Quote:<%= quotes[i].quote %></h5>
        <% } %>
      </fieldset>
      <a href="/"><input type="submit" name="btn" value="Home"></a>
  </body>
</html>
